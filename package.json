{
  "name": "ipai",
  "displayName": "Alejandro Madrid",
  "description": "An interactive vs-code extension for data treatment and visualization using PandasAI",
  "icon": "pandasai-image.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "permissions": [
    "filesystem"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ipai.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.ipai.show",
        "title": "Show I-pAI primary sidebar"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ipai-viewContainer",
          "title": "I-pAI",
          "icon": "src/webview/media/img/pandasai-image.png"
        }
      ]
    },
    "views": {
      "ipai-viewContainer": [
        {
          "id": "ipai.mainContainer",
          "name": "chat",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/jquery": "^3.5.30",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/popper.js": "^1.11.0",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "notebook": "^1.0.1"
  }
}
